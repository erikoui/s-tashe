{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ".",
        "exclude": ["./node_modules","./public","./docs"],
        "includePattern": ".+\\.js(doc)?$"
    },
    "opts": {
        "destination":"./docs",
        "recurse": true,
        "private": true
    },
    
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },

    
  "plugins": ["plugins/markdown"]
}